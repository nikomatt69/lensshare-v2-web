{
  "extends": "@lensshare/config/react.tsconfig.json",
  "compilerOptions": {
    "target":"es2015",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext",
      "webworker"
    ],
    
    "baseUrl": ".",
    "plugins": [{ "name": "next" }],
    "paths": {
      "@components*": ["src/components*"],
      "@lib*": ["src/lib*"],
      "@gql*": ["src/graphql*"],
      "@generated*": ["src/generated*"],
      "@sw/*": ["src/sw/*"]
    },
    
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],

}
